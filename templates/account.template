    <script>
        // å½“å‰ç¼–è¾‘çŠ¶æ€
        let editMode = {};

        // åˆ‡æ¢ç¼–è¾‘æ¨¡å¼
        function toggleEdit(section) {
            const isEditing = editMode[section];
            
            if (isEditing) {
                cancelEdit(section);
            } else {
                enterEditMode(section);
            }
        }

        // è¿›å…¥ç¼–è¾‘æ¨¡å¼
        function enterEditMode(section) {
            editMode[section] = true;
            
            // éšè—æ˜¾ç¤ºå…ƒç´ ï¼Œæ˜¾ç¤ºè¾“å…¥å…ƒç´ 
            const form = document.getElementById(section + 'Form');
            const displays = form.querySelectorAll('.info-value');
            const inputs = form.querySelectorAll('.info-input');
            const actions = document.getElementById(section + '-actions');
            const editBtn = form.closest('.info-section').querySelector('.edit-toggle-btn');
            
            displays.forEach(display => display.style.display = 'none');
            inputs.forEach(input => input.style.display = 'block');
            actions.style.display = 'block';
            editBtn.textContent = 'å–æ¶ˆç¼–è¾‘';
        }

        // å–æ¶ˆç¼–è¾‘
        function cancelEdit(section) {
            editMode[section] = false;
            
            // æ˜¾ç¤ºæ˜¾ç¤ºå…ƒç´ ï¼Œéšè—è¾“å…¥å…ƒç´ 
            const form = document.getElementById(section + 'Form');
            const displays = form.querySelectorAll('.info-value');
            const inputs = form.querySelectorAll('.info-input');
            const actions = document.getElementById(section + '-actions');
            const editBtn = form.closest('.info-section').querySelector('.edit-toggle-btn');
            
            displays.forEach(display => display.style.display = 'block');
            inputs.forEach(input => input.style.display = 'none');
            actions.style.display = 'none';
            editBtn.textContent = 'ç¼–è¾‘';
        }

        // ä¿å­˜åŸºæœ¬ä¿¡æ¯
        function saveBasicInfo() {
            const form = document.getElementById('basicForm');
            const formData = new FormData(form);
            
            fetch('/api/account', {
                method: 'POST',
                body: formData
            })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    showToast('åŸºæœ¬ä¿¡æ¯ä¿å­˜æˆåŠŸ', 'success');
                    updateDisplayValues('basic', formData);
                    cancelEdit('basic');
                    
                    // æ›´æ–°ç»Ÿè®¡å¡ç‰‡
                    if (data.bmi > 0) {
                        updateStatsCards(data);
                    }
                } else {
                    showToast(data.message || 'ä¿å­˜å¤±è´¥', 'error');
                    if (data.errors) {
                        data.errors.forEach(error => showToast(error, 'error'));
                    }
                }
            })
            .catch(error => {
                showToast('ç½‘ç»œé”™è¯¯: ' + error.message, 'error');
            });
        }

        // ä¿å­˜è”ç³»ä¿¡æ¯
        function saveContactInfo() {
            const form = document.getElementById('contactForm');
            const formData = new FormData(form);
            
            fetch('/api/account', {
                method: 'POST',
                body: formData
            })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    showToast('è”ç³»ä¿¡æ¯ä¿å­˜æˆåŠŸ', 'success');
                    updateDisplayValues('contact', formData);
                    cancelEdit('contact');
                } else {
                    showToast(data.message || 'ä¿å­˜å¤±è´¥', 'error');
                    if (data.errors) {
                        data.errors.forEach(error => showToast(error, 'error'));
                    }
                }
            })
            .catch(error => {
                showToast('ç½‘ç»œé”™è¯¯: ' + error.message, 'error');
            });
        }

        // æ›´æ–°æ˜¾ç¤ºå€¼
        function updateDisplayValues(section, formData) {
            const form = document.getElementById(section + 'Form');
            
            for (let [key, value] of formData.entries()) {
                const displayElement = document.getElementById(key + '-display');
                if (displayElement) {
                    if (value) {
                        if (key === 'height' || key === 'weight') {
                            const unit = key === 'height' ? ' cm' : ' kg';
                            displayElement.textContent = value + unit;
                        } else if (key === 'age') {
                            displayElement.textContent = value + 'å²';
                        } else if (key === 'hobbies') {
                            updateHobbiesDisplay(value);
                        } else if (key === 'website' && value) {
                            displayElement.innerHTML = `<a href="${value}" target="_blank">${value}</a>`;
                        } else {
                            displayElement.textContent = value;
                        }
                    } else {
                        displayElement.textContent = 'æœªè®¾ç½®';
                    }
                }
            }
        }

        // æ›´æ–°çˆ±å¥½æ˜¾ç¤º
        function updateHobbiesDisplay(hobbiesStr) {
            const displayElement = document.getElementById('hobbies-display');
            if (hobbiesStr) {
                const hobbies = hobbiesStr.split(',').map(h => h.trim()).filter(h => h);
                const tagsHtml = hobbies.map(hobby => `<span class="hobby-tag">${hobby}</span>`).join('');
                displayElement.innerHTML = `<div class="hobbies-tags">${tagsHtml}</div>`;
            } else {
                displayElement.textContent = 'æœªè®¾ç½®';
            }
        }

        // æ›´æ–°ç»Ÿè®¡å¡ç‰‡
        function updateStatsCards(data) {
            // è¿™é‡Œå¯ä»¥æ›´æ–°BMIå’Œå…¶ä»–ç»Ÿè®¡ä¿¡æ¯çš„æ˜¾ç¤º
            // ä¸ºäº†ç®€åŒ–ï¼Œæˆ‘ä»¬é‡æ–°åŠ è½½é¡µé¢
            setTimeout(() => {
                location.reload();
            }, 1000);
        }

        // å¤´åƒç®¡ç†
        function openAvatarModal() {
            const modal = document.getElementById('avatarModal');
            const input = document.getElementById('avatarInput');
            input.value = '{{.AccountInfo.Avatar}}';
            modal.style.display = 'block';
        }

        function closeAvatarModal() {
            document.getElementById('avatarModal').style.display = 'none';
        }

        function saveAvatar() {
            const avatar = document.getElementById('avatarInput').value.trim();
            if (!avatar) {
                showToast('è¯·è¾“å…¥å¤´åƒå­—ç¬¦', 'error');
                return;
            }

            const formData = new FormData();
            formData.append('avatar', avatar);

            fetch('/api/account/avatar', {
                method: 'POST',
                body: formData
            })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    showToast('å¤´åƒæ›´æ–°æˆåŠŸ', 'success');
                    document.querySelector('.account-avatar').textContent = avatar;
                    closeAvatarModal();
                } else {
                    showToast(data.message || 'å¤´åƒæ›´æ–°å¤±è´¥', 'error');
                }
            })
            .catch(error => {
                showToast('ç½‘ç»œé”™è¯¯: ' + error.message, 'error');
            });
        }

        // æ˜¾ç¤ºToastæ¶ˆæ¯
        function showToast(message, type = 'info') {
            const toast = document.createElement('div');
            toast.className = `toast toast-${type}`;
            toast.textContent = message;
            
            const container = document.getElementById('toast-container');
            if (!container) {
                const newContainer = document.createElement('div');
                newContainer.id = 'toast-container';
                newContainer.className = 'toast-container';
                newContainer.style.cssText = `
                    position: fixed;
                    top: 20px;
                    right: 20px;
                    z-index: 1000;
                `;
                document.body.appendChild(newContainer);
            }
            
            document.getElementById('toast-container').appendChild(toast);
            
            // æ·»åŠ Toastæ ·å¼
            toast.style.cssText = `
                background: ${type === 'success' ? '#28a745' : type === 'error' ? '#dc3545' : '#007bff'};
                color: white;
                padding: 12px 20px;
                border-radius: 6px;
                margin-bottom: 10px;
                box-shadow: 0 2px 8px rgba(0,0,0,0.2);
                animation: slideIn 0.3s ease;
            `;
            
            setTimeout(() => {
                toast.remove();
            }, 3000);
        }

